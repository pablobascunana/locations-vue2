{
  "name": "mylocations-mongodb",
  "version": "1.0.0",
  "description": "Save and add locations in a map",
  "main": "./src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pablobascunana/locations-vue2.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuetify",
    "jest",
    "cypress"
  ],
  "author": "Pablo Bascu√±ana Saiz <https://github.com/pablobascunana> (https://github.com/pablobascunana)",
  "license": "MIT",
  "bugs": "https://github.com/pablobascunana/locations-vue2/issues",
  "homepage": "https://github.com/pablobascunana/locations-vue2",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:unit-debug": "node --inspect-brk node_modules/.bin/vue-cli-service test:unit --no-cache --watch --runInBand",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "jest --coverage",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\""
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-i18n": "^8.25.0",
    "vue-router": "^3.2.0",
    "vue2-google-maps": "^0.10.7",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0",
    "vuex-persistedstate": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.1.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "cypress": "^8.3.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "jest-html-reporters": "^2.1.6",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-i18n": "~2.1.3",
    "vue-cli-plugin-vuetify": "~2.4.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "ignorePatterns": [
      "**/node_modules/**"
    ],
    "rules": {
      "no-debugger": 0
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 10"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,vue}"
    ],
    "coverageDirectory": "./tests/unit/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "pageTitle": "Your location unit test reports",
          "hideIcon": true,
          "publicPath": "./tests/unit/html-reports",
          "filename": "your-location-unit-test-reporters.html",
          "expand": false,
          "openReport": true
        }
      ]
    ]
  }
}
